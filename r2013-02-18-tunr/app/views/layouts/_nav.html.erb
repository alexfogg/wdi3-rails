      <li><%= link_to('Home', root_path) %></li>
      <li class="has-flyout">
        <a href="#">Browse Music</a>
        <a href="#" class="flyout-toggle"></a>
          <ul class="flyout">
            <li><%= link_to('Artists', artists_path) %></li>
            <li><%= link_to('Albums', albums_path) %></li>
