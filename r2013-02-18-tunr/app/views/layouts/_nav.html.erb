            <ul class="flyout">
              <li><%= link_to('Create Mixtape', new_mixtape_path) %></li>
              <%== mixtapenav %>
            </ul>
        </li>
      <% end %>

      <% if is_user? %>
        <li class="has-flyout">
          <a href="#">Albums</a>
          <a href="#" class="flyout-toggle"><span> </span></a>
            <ul class="flyout">
              <%== albumnav %>
